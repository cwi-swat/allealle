Even ∪ Odd = Num

∀ n ∈ Num, n' ∈ Num ∖ n | some (n ⨯ n'[nId'/nId, val'/val]) where val != val'  

∀ n ∈ Num | (some n where (val % 2) = 0) ⇔ n ⊆ Even

∀ n ∈ Num, n' ∈ Num | 
  (some ((n[single/nId] ⨯ n'[double/nId,val2/val]) where val2 = (val * 2))[single,double]) ⇔ 
    n[nId][single/nId] ⨯ n'[nId][double/nId] ⊆ twice

