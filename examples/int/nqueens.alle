Queen (1 :: x:int, y:int) = {<q1,?,?>,<q2,?,?>,<q3,?,?>,<q4,?,?>,<q5,?,?>,<q6,?,?>,<q7,?,?>,<q8,?,?>}

Queen::x >= 1 && Queen::x <=8
Queen::y >= 1 && Queen::y <=8

// Queens cannot be on the same row or column or on the diagonal
forall q:Queen, q':Queen\q | q::x != q'::x && q::y != q'::y && |q::x - q'::x| != |q::y - q'::y|
