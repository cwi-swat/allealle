r3 (nId:id)                 = {<n7>..<n9>}
grid (x:id, y:id, val:int) >= {<n1,n1,6>,<n1,n4,2>,<n1,n8,5>,
                               <n2,n2,1>,<n2,n3,8>,<n2,n5,6>,<n2,n8,2>,
